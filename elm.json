{
    "type": "package",
    "name": "justgook/elm-logic",
    "summary": "An ECS library for Elm. Provides an easy way to build a full game, using common Entity-Component-System architecture.",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": [
        "Logic.System",
        "Logic.Launcher",
        "Logic.GameFlow",
        "Logic.Environment",
        "Logic.Entity",
        "Logic.Component.Singleton",
        "Logic.Component"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.2 <= v < 2.0.0",
        "elm-explorations/linear-algebra": "1.0.3",
        "elm-explorations/benchmark": "1.0.1"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.0.0 <= v < 2.0.0"
    }
}
